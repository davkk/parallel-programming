set terminal eps color enhanced

set output ARG1 . ".eps"

set title "Benchmark - Mandelbrot - Not Parallel"
set xlabel "Size [px]"
set ylabel "Time [s]"

set nokey
set grid

set xtics (32, 64, 128, 256, 512, 1024, 2048, 4096, 8192)
set logscale xy 2

set xrange [16:16384] noextend

set format y "%.2g"

plot ARG1 . ".dat" w p pt 1 lw 4, \
     ARG1 . ".dat" w l lw 1 linecolor 1
